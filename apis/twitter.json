{
  "name": "Twitter (twitter154)",
  "host": "twitter154.p.rapidapi.com",
  "description": "Twitter/X API - search tweets, user profiles, followers, following",
  "rapidapi_key": null,
  "subscription_url": "https://rapidapi.com/dschlenk/api/twitter154",
  "best_practices": {
    "search": "Use section=top for popular tweets, section=latest for recent. Filter with min_likes and min_retweets.",
    "language": "language=en filters English tweets. Use start_date/end_date for date range.",
    "pagination": "Use continuation_token from response to get more results. For search: call /search/search/continuation with token. For replies: check continuation_token in response.",
    "min_retweets": "Note: min_retweets=N returns tweets with >= N retweets. A tweet with 970 RT will be excluded when min_retweets=1000.",
    "replies": "The /tweet/replies endpoint returns results in 'replies' array, not 'results'.",
    "optimal_filters": "Best combo for quality results: min_retweets=100 + min_likes=100 + min_replies=50 + language=en. Reduces spam effectively.",
    "limit": "The API returns max ~20 tweets per call regardless of limit parameter.",
    "trends_woeid": "Trends vary by location. Use WOEID: 1=worldwide, 23424977=USA, 44418=London, 2459115=NYC, 2442047=LA, 23424829=Germany, 638242=Berlin, 609153=Paris, 1118370=Tokyo, 1105779=Sydney. Note: US/DE trends are often identical - only major cities like London/Tokyo/Sydney have distinct trends.",
    "trends_response": "Response is an array: data[0]['trends'][]. Each trend has: name, url, query, tweet_volume (often null).",
    "trends_volume": "tweet_volume is often null. When present, it's the number of posts in last 24h.",
    "search_geo": "Returns ~20 results near a lat/long. Parameters: query, latitude, longitude, range (km, default 32)."
  },
  "endpoints": {
    "search": {
      "path": "/search/search",
      "method": "GET",
      "description": "Search tweets by query",
      "params": {
        "query": {
          "type": "string",
          "required": true,
          "description": "Search query (e.g., openclaw)"
        },
        "section": {
          "type": "string",
          "required": false,
          "description": "top or latest"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of results (default: 20)"
        },
        "language": {
          "type": "string",
          "required": false,
          "description": "Filter by language (en, es, etc.)"
        },
        "start_date": {
          "type": "string",
          "required": false,
          "description": "Start date (YYYY-MM-DD)"
        },
        "end_date": {
          "type": "string",
          "required": false,
          "description": "End date (YYYY-MM-DD)"
        },
        "min_likes": {
          "type": "number",
          "required": false,
          "description": "Minimum likes"
        },
        "min_retweets": {
          "type": "number",
          "required": false,
          "description": "Minimum retweets"
        },
        "min_replies": {
          "type": "number",
          "required": false,
          "description": "Minimum replies"
        },
        "media_type": {
          "type": "string",
          "required": false,
          "description": "Filter by media type (video, photo)"
        }
      },
      "quota_cost": 1
    },
    "search_continuation": {
      "path": "/search/search/continuation",
      "method": "GET",
      "description": "Get more search results using continuation token",
      "params": {
        "continuation_token": {
          "type": "string",
          "required": true,
          "description": "Token from previous search response"
        },
        "query": {
          "type": "string",
          "required": true,
          "description": "Original search query"
        },
        "section": {
          "type": "string",
          "required": false,
          "description": "top or latest"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of results"
        },
        "language": {
          "type": "string",
          "required": false,
          "description": "Filter by language"
        },
        "start_date": {
          "type": "string",
          "required": false,
          "description": "Start date (YYYY-MM-DD)"
        },
        "end_date": {
          "type": "string",
          "required": false,
          "description": "End date (YYYY-MM-DD)"
        },
        "min_likes": {
          "type": "number",
          "required": false,
          "description": "Minimum likes"
        },
        "min_retweets": {
          "type": "number",
          "required": false,
          "description": "Minimum retweets"
        },
        "min_replies": {
          "type": "number",
          "required": false,
          "description": "Minimum replies"
        }
      },
      "quota_cost": 1
    },
    "user_tweets": {
      "path": "/user/tweets",
      "method": "GET",
      "description": "Get user's tweets",
      "params": {
        "username": {
          "type": "string",
          "required": true,
          "description": "Twitter username (without @)"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of tweets (max 100)"
        }
      },
      "quota_cost": 1
    },
    "search_geo": {
      "path": "/search/geo",
      "method": "GET",
      "description": "Search tweets by location/geo",
      "params": {
        "query": {
          "type": "string",
          "required": true,
          "description": "Search query"
        },
        "latitude": {
          "type": "number",
          "required": true,
          "description": "Latitude coordinate"
        },
        "longitude": {
          "type": "number",
          "required": true,
          "description": "Longitude coordinate"
        },
        "range": {
          "type": "number",
          "required": false,
          "description": "Radius in km"
        },
        "section": {
          "type": "string",
          "required": false,
          "description": "top or latest"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of results"
        },
        "language": {
          "type": "string",
          "required": false,
          "description": "Filter by language"
        }
      },
      "quota_cost": 1
    },
    "trends": {
      "path": "/trends/",
      "method": "GET",
      "description": "Get trending topics",
      "params": {
        "woeid": {
          "type": "number",
          "required": false,
          "description": "Where On Earth ID (1 = worldwide, 23424868 = Germany, etc.)"
        }
      },
      "quota_cost": 1
    },
    "tweet_replies": {
      "path": "/tweet/replies",
      "method": "GET",
      "description": "Get replies to a tweet",
      "params": {
        "tweet_id": {
          "type": "string",
          "required": true,
          "description": "Tweet ID"
        }
      },
      "quota_cost": 1
    },
    "tweet_details": {
      "path": "/tweet/details",
      "method": "GET",
      "description": "Get single tweet details by ID",
      "params": {
        "tweet_id": {
          "type": "string",
          "required": true,
          "description": "Tweet ID"
        }
      },
      "quota_cost": 1
    },
    "user_followers": {
      "path": "/user/followers",
      "method": "GET",
      "description": "Get user's followers",
      "params": {
        "username": {
          "type": "string",
          "required": true,
          "description": "Twitter username (without @)"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of followers"
        }
      },
      "quota_cost": 1
    },
    "user_following": {
      "path": "/user/following",
      "method": "GET",
      "description": "Get users that a user follows",
      "params": {
        "username": {
          "type": "string",
          "required": true,
          "description": "Twitter username (without @)"
        },
        "limit": {
          "type": "number",
          "required": false,
          "description": "Number of following"
        }
      },
      "quota_cost": 1
    },
    "user_about": {
      "path": "/user/about",
      "method": "GET",
      "description": "Get user profile info (name, bio, location, etc.)",
      "params": {
        "username": {
          "type": "string",
          "required": true,
          "description": "Twitter username (without @)"
        }
      },
      "quota_cost": 1
    }
  },
  "response_format": {
    "search_tweet": {
      "tweet_id": "string",
      "creation_date": "string",
      "text": "string",
      "media_url": "array",
      "video_url": "string or null",
      "user": {
        "username": "string",
        "name": "string",
        "follower_count": "number",
        "following_count": "number",
        "is_verified": "boolean"
      },
      "favorite_count": "number",
      "retweet_count": "number",
      "reply_count": "number",
      "quote_count": "number"
    },
    "tweet_details": {
      "tweet_id": "string",
      "timestamp": "number (Unix timestamp)",
      "creation_date": "string",
      "text": "string",
      "media_url": "array or null",
      "video_url": "string or null",
      "user": {...},
      "favorite_count": "number",
      "retweet_count": "number",
      "reply_count": "number",
      "quote_count": "number",
      "views": "number",
      "bookmark_count": "number"
    },
    "tweet_replies": {
      "replies": "array of tweet objects (NOT 'results')",
      "continuation_token": "string for pagination"
    },
    "user_profile": {
      "avatar": {"image_url": "string"},
      "core": {"screen_name": "string", "name": "string", "created_at": "string"},
      "profile_banner": {"image_url": "string"},
      "about_profile": {"location_accurate": "boolean"},
      "verification": {"verified": "boolean", "is_blue_verified": "boolean"}
    }
  },
  "example_usage": {
    "search_openclaw": "curl -s 'https://twitter154.p.rapidapi.com/search/search?query=openclaw&section=top&min_likes=100' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "search_geo": "curl -s 'https://twitter154.p.rapidapi.com/search/geo?query=openclaw&latitude=40.7128&longitude=-74.0060&range=50' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "trends_worldwide": "curl -s 'https://twitter154.p.rapidapi.com/trends/?woeid=1' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "trends_london": "curl -s 'https://twitter154.p.rapidapi.com/trends/?woeid=44418' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "trends_tokyo": "curl -s 'https://twitter154.p.rapidapi.com/trends/?woeid=1118370' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "tweet_details": "curl -s 'https://twitter154.p.rapidapi.com/tweet/details?tweet_id=2022046669710491991' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "tweet_replies": "curl -s 'https://twitter154.p.rapidapi.com/tweet/replies?tweet_id=2022046669710491991' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "user_tweets": "curl -s 'https://twitter154.p.rapidapi.com/user/tweets?username=openclawai&limit=10' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'",
    "user_profile": "curl -s 'https://twitter154.p.rapidapi.com/user/about?username=openclawai' -H 'X-RapidAPI-Key: YOUR_KEY' -H 'X-RapidAPI-Host: twitter154.p.rapidapi.com'"
  }
}
